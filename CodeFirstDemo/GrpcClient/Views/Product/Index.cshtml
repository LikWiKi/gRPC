@model List<GrpcServer.Contracts.ProductRequest>

<div class="conatainer p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h1 class="text-primary">Product List</h1>
        </div>
    </div>
    <br /><br />
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Mã
                </th>
                <th>
                    Tên sản phẩm
                </th>
                <th>
                    Giá
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>
                        @obj.Id
                    </td>
                    <td>
                        @obj.Name
                    </td>
                    <td>
                        @obj.Price
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>